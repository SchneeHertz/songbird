{
  "name": "songbird",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "chcp 65001 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "electron.songbird",
    "files": [
      "dist/index.html",
      "dist/assets/*",
      "index.js",
      "preload.js"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.png"
    }
  },
  "dependencies": {
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "sequelize": "^6.25.3",
    "sharp": "^0.31.1",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@egjs/vue3-infinitegrid": "^4.7.1",
    "@vicons/ionicons4": "^0.12.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "axios": "^1.1.3",
    "electron": "20.3.3",
    "electron-builder": "^23.6.0",
    "naive-ui": "^2.33.5",
    "stylus": "^0.59.0",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.0",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2"
  }
}